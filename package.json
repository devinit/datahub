{
  "name": "datahub",
  "version": "1.0.0",
  "config": {
    "api": "http://localhost:3000/graphql"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devinit/datahub"
  },
  "scripts": {
    "clean": "rm -rf ./node_modules/.cache ./next",
    "dev": "BABEL_ENV=development NODE_ENV=development && PORT=4040 node server.js",
    "build": "BABEL_ENV=development NODE_ENV=development npm run clean && next build",
    "start": "NODE_ENV=production PORT=9090 node server.js",
    "build-tools": "BABEL_ENV=tools babel tools --out-dir dist --ignore **/.test.js",
    "pull": "npm run build-tools && node dist/pullApiData/index.js",
    "lessToJs": "npm run build-tools && node dist/lessToJs/index.js",
    "lint": "eslint private server.js --ext .graphql --ext .js && npm run flow",
    "format": "prettier-eslint --write \"components/**/*.js\" \"lib/*.js\" \"pages/*.js\"",
    "flow": "flow",
    "test": "BABEL_ENV=test jest",
    "coverage": "BABEL_ENV=test jest && codecov",
    "storybook": "BABEL_ENV=development start-storybook -c private/.storybook -s public -p 6006 ",
    "build-storybook": "BABEL_ENV=production build-storybook -c private/.storybook -s public -o .out",
    "build-theme": "gulp  --gulpfile ./private/semantic/gulpfile.js && npm run lessToJs",
    "build-theme-w": "gulp watch --gulpfile ./private/semantic/gulpfile.js",
    "qql-schema": "apollo-codegen introspect-schema $npm_package_config_api  --output schema.json",
    "gql-flow": "apollo-codegen generate private/**/*.graphql --schema schema.json --target flow --output types/schema.flow.js",
    "gql": "npm run qql-schema &&  npm run gql-flow"
  },
  "peerDependencies": {
    "graphql": "^0.10.3",
    "apollo-client": "^1.9.0-1"
  },
  "dependencies": {
    "@devinit/charts": "^1.2.9",
    "apollo-client": "^1.9.0-1",
    "apollo-fetch": "^0.3.0",
    "approximate-number": "^2.0.0",
    "babel-cli": "^6.24.1",
    "babel-plugin-inline-import-graphql-ast": "^2.0.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-inline-imports-commonjs": "^1.2.0",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "compression": "^1.6.2",
    "d3": "^4.9.1",
    "dragdealer": "^0.9.9",
    "express": "^4.15.3",
    "glamor": "^2.20.25",
    "glamorous": "^3.23.4",
    "graphql": "^0.10.3",
    "isomorphic-fetch": "^2.2.1",
    "localforage": "^1.5.0",
    "mapbox-gl": "^0.38.0",
    "next": "^3.0.1-beta.18",
    "next-routes": "^1.0.40",
    "prop-types": "^15.5.10",
    "ramda": "^0.24.1",
    "raw-loader": "^0.5.1",
    "react": "^15.6.1",
    "react-apollo": "^1.4.8",
    "react-dom": "^15.6.1",
    "react-no-ssr": "^1.1.0",
    "react-redux": "^5.0.5",
    "redux": "^3.7.0",
    "redux-persist": "^4.8.1",
    "semantic-ui-react": "^0.71.1",
    "sw-precache-webpack-plugin": "^0.11.4"
  },
  "author": "epicallan.al@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@storybook/addon-storyshots": "^3.1.9",
    "@storybook/react": "^3.1.9",
    "apollo-codegen": "^0.15.2",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.6.0",
    "css-to-js-object": "^0.0.3",
    "enzyme": "^2.9.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-graphql": "^1.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "7.0.1",
    "flow-bin": "^0.51.0",
    "flow-remove-types": "^1.2.1",
    "fs-extra": "^4.0.0",
    "git-pre-hooks": "^1.2.0",
    "gulp": "^3.9.1",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "less": "^2.7.2",
    "prettier": "^1.4.4",
    "prettier-eslint-cli": "^4.0.3",
    "pretty-format": "^20.0.3",
    "react-test-renderer": "^15.5.4",
    "semantic-ui": "^2.2.10"
  },
  "git-pre-hooks": {
    "pre-push": [
      "npm run lint"
    ]
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/private/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/private/__mocks__/styleMock.js"
    }
  },
  "engines": {
    "node": ">=7"
  }
}
