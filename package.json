{
  "name": "datahub",
  "version": "2.2.1",
  "config": {
    "api": "http://212.111.41.68:3000/graphql",
    "old_datahub": "http://212.111.41.68:8888"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devinit/datahub"
  },
  "author": "epicallan.al@gmail.com",
  "license": "ISC",
  "scripts": {
    "clean": "rm -rf ./node_modules/.cache ./next",
    "dev": "npm run build-workers && BABEL_ENV=development NODE_ENV=development PORT=4040 node server.js",
    "build": "BABEL_ENV=development NODE_ENV=development npm run clean && next build",
    "start": "NODE_ENV=production PORT=8080 node server.js",
    "build-tools": "BABEL_ENV=tools babel tools --out-dir dist --ignore **/.test.js",
    "build-fragment": "npm run build-tools && node dist/graphql/index.js",
    "pull": "npm run build-tools && node dist/pullApiData/index.js",
    "precache": "npm run build-tools && node dist/precache/index.js",
    "critical-css": "npm run build-tools && node dist/criticalCss/index.js",
    "lessToJs": "npm run build-tools && node dist/lessToJs/index.js",
    "lint": "eslint private server.js --ext .grabphql --ext .js && npm run flow",
    "format": "prettier-eslint --write \"private/**/*.js\" \"tools/**/*.js\" \"pages/*.js\"",
    "format-p": "prettier-eslint --write \"private/components/templates/UnbundlingAid/index.js\"",
    "deploy": "git checkout master && git rebase dev && git push origin master && git push origin dev && git checkout dev",
    "patch": "release-it --no-npm.publish -i patch -n",
    "feature": "release-it --no-npm.publish -i minor -n",
    "breaking": "release-it --no-npm.publish -i major -n",
    "multilaterals": "node tools/buildScripts/multilaterals.js",
    "flow": "flow",
    "test": "BABEL_ENV=test jest",
    "analyze": "ANALYZE=1 next build",
    "coverage": "BABEL_ENV=test jest -u && codecov",
    "storybook": "BABEL_ENV=development start-storybook -c private/.storybook -s public -p 6006 ",
    "build-storybook": "BABEL_ENV=production build-storybook -c private/.storybook -s public -o .out",
    "build-theme": "gulp  --gulpfile ./private/semantic/gulpfile.js",
    "build-theme-w": "gulp watch --gulpfile ./private/semantic/gulpfile.js",
    "build-workers": "BABEL_ENV=production node tools/buildScripts/worker.js",
    "vendor": "node tools/buildScripts/vendor.js",
    "qql-schema": "apollo-codegen introspect-schema $npm_package_config_api  --output schema.json",
    "gql-flow": "apollo-codegen generate **/*.graphql --schema schema.json --target flow --output types/schema.flow.js",
    "gql": "npm run qql-schema &&  npm run gql-flow"
  },
  "dependencies": {
    "@devinit/charts": "1.6.40",
    "apollo-client-preset": "^1.0.6",
    "apollo-fetch": "^0.7.0",
    "compression": "^1.7.1",
    "d3": "^4.11.0",
    "dragdealer": "^0.10.0",
    "express": "^4.16.2",
    "fs-extra": "^5.0.0",
    "glamor": "^2.20.40",
    "glamorous": "^4.11.0",
    "graphql": "^0.12.3",
    "intersection-observer": "^0.5.0",
    "isomorphic-fetch": "^2.2.1",
    "localforage": "^1.5.3",
    "lru-cache": "^4.1.1",
    "mapbox-gl": "^0.44.0",
    "next": "^4.2.3",
    "prettier": "^1.7.4",
    "ramda": "^0.25.0",
    "react": "^16.0.0",
    "react-apollo": "^2.0.0",
    "react-beautiful-dnd": "^4.0.0",
    "react-dom": "^16.0.0",
    "react-intersection-observer": "^3.0.2",
    "react-no-ssr": "^1.1.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.0",
    "semantic-ui-react": "^0.78.0"
  },
  "devDependencies": {
    "@storybook/addon-storyshots": "^3.3.10",
    "@storybook/react": "^3.3.10",
    "apollo-codegen": "^0.18.3",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-inline-import-graphql-ast": "^2.0.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-ramda": "^1.4.3",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-inline-imports-commonjs": "^1.2.0",
    "babel-preset-env": "^1.6.1",
    "css-to-js-object": "0.0.3",
    "csso": "^3.1.1",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-graphql": "^1.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "flow-bin": "^0.59.0",
    "flow-remove-types": "^1.2.1",
    "git-pre-hooks": "^1.2.0",
    "gulp": "^3.9.1",
    "jest": "^22.0.6",
    "jest-cli": "^22.0.6",
    "less": "^2.7.2",
    "prettier-eslint-cli": "^4.0.3",
    "pretty-format": "^22.0.6",
    "react-test-renderer": "^16.0.0",
    "release-it": "^6.0.0",
    "semantic-ui": "^2.2.13",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "usus": "^1.6.0",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.0"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "setupTestFrameworkScriptFile": "<rootDir>/private/lib/jest.config.js",
    "collectCoverage": true,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/private/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/private/__mocks__/styleMock.js"
    }
  },
  "engines": {
    "node": ">=8.5"
  }
}
