{
  "env": {
    "development": {
      "presets": "next/babel"
    },
    "production": {
      "presets": "next/babel"
    },
    "test": {
      "presets": [
        ["env", {
          "modules": "commonjs"
        }],
        "next/babel"
      ]
    },
    "tools": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": "current"
            }
          }
        ]
      ]
    }
  },
  "plugins": [
    "transform-flow-strip-types",
    "transform-async-generator-functions",
    "babel-plugin-inline-import-graphql-ast", ["module-resolver", {
      "root": ["private", "public/semantic", "public/img"],
      "alias": {
        "package.json": "./package.json"
      }
    }]
  ]
}
