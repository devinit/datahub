{
  "name": "@devinit/dh-ui",
  "version": "1.0.4",
  "description": "for creating datahub ui components",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "copy-css": "cp node_modules/@devinit/charts/dist/di-charts.min.css static/ && cp node_modules/mapbox-gl/dist/mapbox-gl.css static/",
    "lint": "tslint -c tslint.json 'src/**/*.tsx' 'src/**/*.ts'",
    "storybook": "start-storybook -p 6006 -s static",
    "patch": "release-it -i patch -n",
    "feature": "release-it -i minor -n",
    "breaking": "release-it -i major -n",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "ui",
    "datahub",
    "storybook"
  ],
  "author": "epicallan.al@gmail.com",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devinit/datagub.git"
  },
  "dependencies": {
    "@devinit/charts": "^1.6.49",
    "@devinit/datahub-api": "^1.0.2",
    "@devinit/dh-base": "^1.0.4",
    "glamor": "^2.20.40",
    "glamorous": "^4.11.4",
    "intersection-observer": "^0.5.0",
    "mapbox-gl": "^0.44.0",
    "next": "^5.0.0",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-beautiful-dnd": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-intersection-observer": "^3.0.3",
    "react-no-ssr": "^1.1.0",
    "semantic-ui-react": "0.78.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.3.14",
    "@storybook/addon-links": "3.3.14",
    "@storybook/addon-storyshots": "3.3.14",
    "@storybook/addons": "3.3.14",
    "@storybook/react": "3.3.14",
    "@types/react-beautiful-dnd": "4.0.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "react-test-renderer": "16.2.0",
    "ts-jest": "22.4.0",
    "typescript": "2.7.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/jest/config.js",
    "coverageDirectory": "./coverage/",
    "testRegex": ".*\\.test\\.(ts|tsx)$",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/__mocks__/fileMock.js",
			"\\.(css|less)$": "<rootDir>/jest/__mocks__/styleMock.js"
		},
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
