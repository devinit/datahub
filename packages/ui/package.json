{
  "name": "@devinit/dh-ui",
  "version": "1.0.2",
  "description": "for creating datahub ui components",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "copy-css": "cp node_modules/@devinit/charts/dist/di-charts.min.css static/ && cp node_modules/mapbox-gl/dist/mapbox-gl.css static/",
    "lint": "tslint -c tslint.json 'src/**/*.tsx' 'src/**/*.ts'",
    "storybook": "start-storybook -p 6006 -s static",
    "patch": "release-it -i patch -n",
    "feature": "release-it -i minor -n",
    "breaking": "release-it -i major -n",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "ui",
    "datahub",
    "storybook"
  ],
  "author": "epicallan.al@gmail.com",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devinit/datagub.git"
  },
  "peerDependencies": {
    "next": "^5.0.0"
  },
  "dependencies": {
    "@devinit/charts": "^1.6.49",
    "@devinit/datahub-api": "^1.0.2",
    "@devinit/dh-base": "^1.0.3",
    "glamor": "^2.20.40",
    "glamorous": "^4.11.4",
    "intersection-observer": "^0.5.0",
    "mapbox-gl": "^0.44.0",
    "next": "^5.0.0",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-beautiful-dnd": "^4.0.1",
    "react-dom": "^16.2.0",
    "react-intersection-observer": "^3.0.3",
    "react-no-ssr": "^1.1.0",
    "semantic-ui-react": "0.78.2"
  },
  "devDependencies": {
    "@storybook/addon-storyshots": "3.3.13",
		"@storybook/addon-actions": "3.3.13",
		"@storybook/addon-links": "3.3.13",
		"@storybook/addons": "3.3.13",
		"@storybook/react": "3.3.13",
    "@types/react-beautiful-dnd": "3.0.1",
    "typescript": "2.7.2",
    "ts-jest": "22.0.4"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
