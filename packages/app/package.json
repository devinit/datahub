{
  "name": "@devinit/dh-app",
  "version": "3.0.5",
  "config": {
    "API": "http://212.111.41.68:9090/graphql",
    "OLD_DATAHUB_URL": "http://212.111.41.68:8888"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devinit/datahub"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "epicallan.al@gmail.com",
  "license": "ISC",
  "scripts": {
    "build": "tsc",
    "test": "jest -u",
    "dev": "nodemon src/server/index.ts",
    "build:next": "next build",
    "analyze": "ANALYZE=1 next build",
    "build:workers": "ts-node src/scripts/worker.ts",
    "build-fragment": "npm run build && node lib/apollo/fragment.js",
    "start": "NODE_ENV=production PORT=3000 node lib/server/index.js",
    "cp-assets": "cp -R ../ui/static/* static/",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "storybook": "start-storybook -p 6005 -s static",
    "gql-schema": "apollo-codegen introspect-schema $npm_package_config_api  --output schema.json",
    "gql-ts": "apollo-codegen generate ./src/**/query.graphql.ts --schema schema.json --target typescript --output ./src/types.ts",
    "types": "npm run gql-schema && npm run gql-ts && npm run cp-types-ui",
    "cp-types-ui": "cp ./src/types.ts ../ui/src/gql-types.ts",
    "patch": "release-it -i patch -n",
    "feature": "release-it -i minor -n",
    "breaking": "release-it -i major -n"
  },
  "dependencies": {
    "@devinit/datahub-api": "1.0.4",
    "@devinit/dh-base": "^1.0.4",
    "@devinit/dh-ui": "^1.0.4",
    "@devinit/graphql-next": "0.1.17",
    "@zeit/next-css": "0.1.2",
    "apollo-client-preset": "1.0.8",
    "compression": "1.7.2",
    "express": "4.16.2",
    "glamor": "2.20.40",
    "glamorous": "4.11.6",
    "graphql": "0.13.1",
    "graphql-tag": "2.8.0",
    "intersection-observer": "0.5.0",
    "isomorphic-fetch": "2.2.1",
    "localforage": "1.5.7",
    "lru-cache": "4.1.1",
    "next": "5.0.1-canary.9",
    "ramda": "0.25.0",
    "react": "16.2.0",
    "react-apollo": "2.0.4",
    "react-dom": "16.2.0",
    "react-redux": "5.0.7",
    "redux": "3.7.2",
    "semantic-ui-react": "0.78.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.3.14",
    "@storybook/addon-links": "3.3.14",
    "@storybook/addon-storyshots": "3.3.14",
    "@storybook/addons": "3.3.14",
    "@storybook/react": "3.3.14",
    "@zeit/next-typescript": "0.0.10",
    "apollo-codegen": "0.18.8",
    "enzyme-adapter-react-16": "1.1.1",
    "nodemon": "1.17.1",
    "react-test-renderer": "16.2.0",
    "ts-jest": "22.4.0",
    "ts-node": "5.0.0",
    "typescript": "2.7.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/jest/config.js",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "testRegex": ".*\\.test\\.(ts|tsx)$",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/jest/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
