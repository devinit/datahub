{
  "compilerOptions": {
    "outDir": "lib",
    "noEmitOnError": false,
    "sourceMap": false,
    "target": "es5",
    "moduleResolution": "node",
    "module": "commonjs",
    "lib": [
      "dom",
      "es2017"
    ],
    "types": [
      "semantic-ui-react"
    ],
    "declaration": false,
    "removeComments": true,
    "noLib": false,
    "watch": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "strict": true,
    "strictFunctionTypes": false,
    "skipLibCheck": false,
    "jsx": "react",
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "exclude": [
    "data",
    "node_modules"
  ],
  "include": [
    "src",
    "pages"
  ]
}
